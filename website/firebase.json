{
    "hosting": {
        "public": "dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/api",
                "run": {
                    "serviceId": "faxer-server",
                    "region": "europe-west1"
                }
            }
        ]
    }
}
